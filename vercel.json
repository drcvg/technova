{
    "version": 2,
    "builds": [
      {
        "src": "servidor/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
  }
  